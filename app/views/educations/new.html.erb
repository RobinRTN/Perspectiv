
 <%= simple_form_for(@education) do |f| %>
    <%= f.input :institution_id, collection: @institutions %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :degree_level %>
    <%= f.input :field %>
    <%= f.submit 'Create education', class: "btn btn-primary"%>
  <% end %>
